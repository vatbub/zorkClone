 <settings>
	<localRepository/>
	<interactiveMode/>
	<usePluginRegistry/>
	<offline/>
	<pluginGroups/>
	<servers>
		<server>
			<id>bintray-vatbub-fokprojectsReleases</id>
			<username>${env.CI_DEPLOY_USERNAME}</username>
			<password>${env.CI_DEPLOY_PASSWORD}</password>
		</server>
		<server>
			<id>bintray-vatbub-fokprojectsSnapshots</id>
			<username>${env.CI_DEPLOY_USERNAME}</username>
			<password>${env.CI_DEPLOY_PASSWORD}</password>
		</server>
		<server>
			<id>s3.site</id>
			<username>${env.AWS_ACCESS_KEY_ID}</username>
			<password>${env.AWS_SECRET_KEY}</password>
		</server>
	</servers>
	<mirrors/>
	<proxies/>
	<profiles>
		<profile>
			<id>disableCodehaus</id>
			<repositories>
				<repository>
					<id>codehaus</id>
					<url>https://nexus.codehaus.org/snapshots</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>false</enabled>
					</releases>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>codehaus-snapshots</id>
					<url>https://nexus.codehaus.org/snapshots</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>false</enabled>
					</releases>
				</pluginRepository>
			</pluginRepositories>
			<properties>
				<srcclrKey>${env.srcclrKey}</srcclrKey>
				<coverallsKey>${env.coverallsKey}</coverallsKey>
				<ojoUsername>${env.OJO_USERNAME}</ojoUsername>
				<ojoPassword>${env.OJO_PASSWORD}</ojoPassword>
			</properties>
		</profile>
	</profiles>

	<activeProfiles>
		<activeProfile>disableCodehaus</activeProfile>
	</activeProfiles>
</settings>
